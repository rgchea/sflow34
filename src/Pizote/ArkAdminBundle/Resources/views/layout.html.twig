    <body class="cover">

        <div class="wrapper">
            <!-- Demo Bar -->
            <div id="layout_options">
                <a href="#" class="options-handle"><i class="fa fa-gear"></i></a>
                <h5>Opciones de Pantalla</h5>
                <label>
                    <input type="checkbox" id="fixed_header" />
                    Encabezado fijo
                </label>
                <label>
                    <input type="checkbox" id="fixed_container" />
                    En contenedor
                </label>
            </div>
            <!-- END: Demo Bar -->
		    {{ include('PizoteArkAdminBundle:Partials:header.html.twig') }}
		    {{ include('PizoteArkAdminBundle:Partials:aside.html.twig') }}


            <!-- BODY -->
            <div class="body">

                <section class="content">
                    
					<ol class="breadcrumb">
			            {% block breadcrumb %}
			            	<li class="active"><a href="{{ path('solucel_admin_homepage') }}">Home</a></li>
			            {% endblock %}						
					    
					</ol>

					
					<!-- CONTENT -->
					<div class="main-content">
						{% block body %}
						
				              {% for flashMessage in app.session.flashbag.get('warning') %}
				                <div class="alert alert-warning fade in">
				                  <button class="close" data-dismiss="alert">
				                    ×
				                  </button>
				                  <i class="fa-fw fa fa-warning"></i>
				                  <strong>Warning</strong> {{ flashMessage|raw }}
				                </div>
				              {% endfor %}
				
				              {% for flashMessage in app.session.flashbag.get('success') %}
				                <div class="alert alert-success fade in">
				                  <button class="close" data-dismiss="alert">
				                    ×
				                  </button>
				                  <i class="fa-fw fa fa-check"></i>
				                  <strong>Success</strong> {{ flashMessage|raw }}
				                </div>
				              {% endfor %}
				
				              {% for flashMessage in app.session.flashbag.get('info') %}
				                <div class="alert alert-info fade in">
				                  <button class="close" data-dismiss="alert">
				                    ×
				                  </button>
				                  <i class="fa-fw fa fa-info"></i>
				                  <strong>Info!</strong> {{ flashMessage |raw }}
				                </div>
				              {% endfor %}
				
				              {% for flashMessage in app.session.flashbag.get('danger') %}
				                <div class="alert alert-danger fade in">
				                  <button class="close" data-dismiss="alert">
				                    ×
				                  </button>
				                  <i class="fa-fw fa fa-times"></i>
				                  <strong>Info!</strong> {{ flashMessage|raw }}
				                </div>
				              {% endfor %}						
						{% endblock %}
					
					    
					</div>
					<!-- END: CONTENT -->
                </section>
            </div>
            <!-- END: BODY -->
